AED2.add_child(APA)
IP.add_child(SO)
IP.add_child(LPP)
IP.add_child(ES)
IP.add_child(CG)
IP.add_child(MPC)
IP.add_child(PFC1)
PFC1.add_child(PFC2)



root.add_child(FE1)


# Imprimir a árvore com no máximo X subárvores
print_tree(root, max_levels=150, spacing=4)

